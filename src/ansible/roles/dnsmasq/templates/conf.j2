# fichier de configuration de dnsmasq

domain-needed
expand-hosts
bogus-priv

interface=eth1
cache-size=256

# réseau local 192.168.50.0/24
dhcp-range=lan1,192.168.50.10,192.168.50.100,12h

# réseau distant 192.168.0.0/24
dhcp-range=lan2,192.168.50.0.10,192.168.0.100,255.255.255.0.12h
dhcp-option=lan2,3,192.168.0.1
