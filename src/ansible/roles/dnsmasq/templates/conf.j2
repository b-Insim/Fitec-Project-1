# fichier de configuration de dnsmasq

domain-needed
expand-hosts
bogus-priv

interface=eth1
cache-size=256

# reseau local 192.168.50.0/24
dhcp-range=192.168.50.10,192.168.50.100,12h

# reseau distant 192.168.0.0/24
dhcp-range=lan2,192.168.50.0.10,192.168.0.100,255.255.255.0.12h
dhcp-option=lan2,3,192.168.0.1


# addresse IP FIXE POUR LA machine
dhcp-host=00:40:F4:6c:43:AF ,machinefixe,192.168.50.10
